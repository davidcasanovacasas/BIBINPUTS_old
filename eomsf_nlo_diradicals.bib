@article{NLO:Prasad:1996,
  author={J.D. Bhawalkar and G.S. He and P.N. Prasad},
  title={Nonlinear multiphoton processes in organic and polymeric materials},
  journal={Rep. Prog. Phys.},
  volume={59},
  number={9},
  pages={1041},
  url={http://stacks.iop.org/0034-4885/59/i=9/a=001},
  year={1996},
  abstract={For several decades there has been extensive research in the area of multiphoton spectroscopy. However, multiphoton processes have not found widespread applications due to the relatively low multiphoton absorption cross sections of most materials. A new generation of multifunctional organic materials with large multiphoton absorption cross sections has opened up a number of unique applications in photonics and biophotonics. Two-photon pumped upconversion lasing, multiphoton absorption-induced optical power limiting, multiphoton laser scanning microscopy, and two-photon three-dimensional optical data storage are some of the recent photonic applications of multiphoton processes highlighted in this article. Two-photon photodynamic therapy is another promising application to biophotonics which is also discussed here. .}
}

